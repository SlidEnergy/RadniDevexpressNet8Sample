{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Local": {
    "Authority": "{AUTHORITY}",
    "ClientId": "{CLIENT ID}"
  },
  "AuthenticationOptions": {
    "ApiName": "Authentication",
    "Endpoints": {
      "ValidateJwtTokenUri": "jwt/validate",
      "RefreshJwtTokenUri": "jwt/refresh",
      "GetAccessScopeUri": "access-scope",
      "SignInUri": "login"
    }
  },
  "HttpClientOptions": {
    "ApiCollection": [
      {
        "Name": "Authentication",
        "BaseAddress": "https://localhost:44375/andromedaWebApi/authentication/"
      },
      {
        "Name": "MyApplicationApi",
        "BaseAddress": "https://localhost:44375/andromedaWebApi/"
      }
    ]
  },
  "DynamicDataProviderOptions": {
    "BaseAddress": "https://localhost:44375/andromedaWebApi/"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Logs\\log.txt",
          "outputTemplate": "{Timestamp} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "LoggerOutputPath": "logs/.txt"
}
